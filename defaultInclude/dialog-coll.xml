                <imagequery
                        jcr:primaryType="cq:Widget"
                        path="/apps/platform/components/modules/util/query/query-pages.infinity.json"
                        xtype="cqinclude"/>
                <selector
                        jcr:primaryType="cq:Widget"
                        path="/apps/platform/components/modules/util/selector/selector-pages.infinity.json"
                        xtype="cqinclude"/>
            </items>
        </tabs>
    </items>
</jcr:root>